<div class="hero-unit">
  <h2 class="name"><%= ware.name %></h2>
  <%- linkTo('Edit this ware', editWarePath(params.id), {class: 'btn pull-right'}); %>
  <%- linkTo('Back to list', warePath(''), {class: 'btn pull-right'}); %>
</div>

<h2>Ware Properties</h2>
<% for(var i in ware) { -%>
<% if(i !== 'saved') { -%>
<div class="row list-item">
  <div class="span4">
    <h3><%= i %></h3>
  </div>
  <div class="span8">
    <h3><i class="icon-list-alt"></i><span class="<%= i %>"><%= ware[i] %></span></h3>
  </div>
</div>
<% } -%><% } -%>